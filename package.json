{
  "private": true,
  "name": "npm-app",
  "productName": "npm",
  "version": "1.0.0",
  "description": "npm as an app",
  "main": "app/main.js",
  "dependencies": {
    "flux": "^3.1.3",
    "immutable": "^3.8.2",
    "react": "^16.4.2",
    "react-desktop": "^0.3.7",
    "react-dom": "^16.4.2",
    "react-select": "^2.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "electron": "^2.0.6",
    "electron-packager": "^12.1.0",
    "standard": "^11.0.1"
  },
  "scripts": {
    "test": "standard",
    "watch": "watchify lib/index.jsx -o app/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "babel": "browserify lib/index.jsx -o app/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "start": "electron .",
    "build": "electron-packager . --platform=darwin --overwrite --arch=x64 --out=dist --icon=app/icon.icns"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/watilde/npm-app.git"
  },
  "keywords": [
    "electron",
    "electron-app",
    "npm"
  ],
  "author": "Daijiro Wachi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/watilde/npm-app/issues"
  },
  "homepage": "https://github.com/watilde/npm-app#readme"
}
